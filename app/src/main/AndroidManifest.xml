        android:theme="@style/AppTheme">
        <activity android:name=".PrincipaleActivite">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CampusActivite">
        </activity>
        <activity
            android:name=".ConnexionActivite">
        </activity>
        <activity
            android:name=".InscriptionActivite">
        </activity>
        <activity
            android:name=".PrendrePhotoActivite">
        </activity>
        <activity
            android:name=".EndroitActivite">
        </activity>
    </application>

</manifest>
