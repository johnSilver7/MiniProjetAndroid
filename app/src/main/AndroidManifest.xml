<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.m2dl.miniprojet.miniandroidter.activites">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Clean Campus"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PrincipaleActivite"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CampusActivite"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:name=".ConnexionActivite"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:name=".InscriptionActivite"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:name=".PrendrePhotoActivite"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:name=".EndroitActivite"
            android:screenOrientation="sensorPortrait">
        </activity>
    </application>

</manifest>
